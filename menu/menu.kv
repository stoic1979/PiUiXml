#:import lv kivy.uix.listview
#:import la kivy.adapters.listadapter
#:import get_products_by_category products_parser.get_products_by_category

<ListItemButton>:
    selected_color: 255,255,255,1
    deselected_color:  255,255,255,1
    color: 0, 0, 0, 1
    font_size: '12sp'


<Test>:
    size_hint: 1, 1
    #pos_hint: {'center_x': .5, 'center_y': .5}
    tab_pos: 'top_mid'
    do_default_tab: False

###################################################################
#                                                                 #
#                   SYSTEM SCREEN                                 #
#                                                                 #
###################################################################
    TabbedPanelItem:
        text: 'System'
        BoxLayout:
            orientation: 'vertical'
            canvas.before:
                Color:
                    rgba: 255,255,255, 1
                Rectangle:
                    pos: self.pos
                    size: self.size
            Label:
                size_hint: 1.0,.2
                text: ''
            Label:
                size_hint: 1.0,.25
                text: 'UPTIME  365:11:15:30'
                color: 0, 0, 0, 1

            Label:
                size_hint: 1.0,.25
                text: 'LAST  SERVER  CONTACT  365:11:15:30'
                color: 0, 0, 0, 1

            BoxLayout:
                orientation: 'horizontal'
                size_hint: 1, .7
                padding: 30
                Label:
                    text: ''
                Label:
                    size_hint: .3, .5
                    text: 'API KEY'
                    color: 0, 0, 0, 1
                TextInput:
                    size_hint: .5, .5
                    text: ''
                Label:
                    text: ''
                Label:
                    size_hint: .2, 1
                    text: ''
            AnchorLayout:
                Button:
                    size_hint: .15, .15
                    text: "PI SYSTEM UPDATE"
                    color: 0, 0, 0, 1

            Label:
                text: ''
            Label:
                text: ''



